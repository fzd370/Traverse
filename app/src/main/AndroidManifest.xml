            android:theme="@style/M_Places" />
        <activity
            android:name=".Jaipur"
            android:theme="@style/Jaipur" />
        <activity
            android:name=".Mussoorie"
            android:theme="@style/Jaipur" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>